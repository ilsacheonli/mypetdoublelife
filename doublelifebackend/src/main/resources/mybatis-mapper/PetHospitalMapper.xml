<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mypet.doublelifebackend.dao.HospitalDAO">
    <select id="selectList" parameterType="com.mypet.doublelifebackend.entity.PetHospital" resultType="com.mypet.doublelifebackend.entity.PetHospital">
