<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mypet.doublelifebackend.repository.SalonRepository">
    <select id="getSalonList" resultType="com.mypet.doublelifebackend.vo.PetSalonVO">
        SELECT num
             , name
             , tel
             , address2
             , latitude
             , longitude
        FROM PETSALON
    </select>
</mapper>